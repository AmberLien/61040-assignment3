{
  "name": "medilex-demo",
  "version": "1.0.0",
  "description": "An application with a chatbot for summarizing and answering questions about medicine",
  "main": "dist/llmchat-tests.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/llmchat-tests.js",
    "dev": "ts-node --transpile-only llmchat-tests.ts",
    "test": "npm run build && node dist/llmchat-tests.js",
    "newChat": "npm run build && node -e \"require('./dist/llmchat-tests.js').testNewChat()\"",
    "followup": "npm run build && node -e \"require('./dist/llmchat-tests.js').testFollowup()\"",
    "renameAndDelete": "npm run build && node -e \"require('./dist/llmchat-tests.js').testRenameAndDeleteChat()\""
  },
  "keywords": ["gemini", "ai", "schedule", "student", "demo", "typescript"],
  "author": "Student Demo",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  }
}
